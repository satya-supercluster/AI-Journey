   "source": [
    "import numpy as np\n",
    "data = np.loadtxt(\"./dataset.csv\",delimiter=\",\",skiprows=1)\n",
    "col1 = data[:,0]\n",
    "print(\"Mean: \",col1.mean())"
   ]
